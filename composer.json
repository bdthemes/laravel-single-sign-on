{
    "name": "bdthemes/laravel-single-sign-on",
    "description": "Laravel wrapper around OAuth2 libraries.",
    "keywords": ["oauth", "laravel","bdthemes"],
    "license": "MIT",
    "homepage": "https://bdthemes.com",
    "support": {
        "issues": "https://github.com/bdthemes/laravel-single-sign-on/issues",
        "source": "https://github.com/bdthemes/laravel-single-sign-on"
    },
    "authors": [
        {
            "name": "Anwar",
            "email": "me@anwar.dev"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "BdThemes\\SingleSignOn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BdThemes\\SingleSignOn\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "BdThemes\\SingleSignOn\\SingleSignOnServiceProvider"
            ],
            "aliases": {
                "bdthemesSSO": "BdThemes\\SingleSignOn\\Facades\\SingleSignOn"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
